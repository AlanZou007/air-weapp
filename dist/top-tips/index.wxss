.air-message{display:block;position:fixed;top:0;left:0;right:0;width:100%;min-height:32px;line-height:2.3;background:#1890ff;color:#fff;text-align:center;font-size:28rpx;z-index:888;opacity:0;transform:translateZ(0) translateY(-100%);transition:all .4s ease-in-out}.air-message-show{transform:translateZ(0) translateY(0);opacity:1}.air-message-default{background:#1890ff}.air-message-success{background:#52c41a}.air-message-warning{background:#fa8c16}.air-message-error{background:#ed3f14}