.air-cell{position:relative;padding:12px 15px;display:flex;background:#fff;align-items:center;line-height:1.5;font-size:28rpx}.air-cell::after{content:' ';position:absolute;pointer-events:none;box-sizing:border-box;transform-origin:center;top:auto;left:15px;right:0;bottom:0;transform:scaleY(.5);border-bottom:1px solid #ccc}.air-cell-last::after{display:none}.air-cell-icon{margin-right:5px}.air-cell-icon:empty{display:none}.air-cell-bd{flex:1}.air-cell-text{line-height:24px}.air-cell-desc{line-height:1.2;font-size:24rpx;color:#999}.air-cell-ft{position:relative;text-align:right}.air-cell-access .air-cell-ft{padding-right:13px}.air-cell-access .air-cell-ft::after{content:" ";display:inline-block;width:6px;height:6px;position:absolute;top:50%;right:2px;border-width:2px 2px 0 0;border-color:#ccc;border-style:solid;transform:translateY(-50%) matrix(.71,.71,-.71,.71,0,0)}