.air-stepper{color:#666}.air-stepper view{display:inline-block;line-height:20px;padding:5px 0;text-align:center;min-width:40px;box-sizing:border-box;vertical-align:middle;font-size:24rpx;border:1rpx solid #ccc}.air-stepper-minus{border-radius:2px 0 0 2px;border-right:none!important}.air-stepper-plus{border-radius:0 2px 2px 0;border-left:none!important}.air-stepper-text{display:inline-block;text-align:center;vertical-align:middle;height:30px;width:40px;min-height:auto;font-size:24rpx;line-height:30px;border:1rpx solid #ccc}.air-stepper-disabled{color:#bbbec4;background:#ccc}